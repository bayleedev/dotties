[user]
  name = Baylee Schmeisser
  email = github@baylee.dev
[core]
  editor = nvim
[alias]
  cb = !git rev-parse --abbrev-ref head | pbcopy
  deleteBranches = !git branch --merged | egrep -v '(master|gh-pages)' | xargs git branch -d
  wip = !git commit --no-verify -m \"wip [skip ci]\"
  retry = !git wip --amend
  aa = add --all
  ap = add --patch
  ci = commit -v
  ca = commit -v --amend
  co = checkout
  pf = push --force-with-lease
  st = status
  lg = log --stat --summary --show-signature
  cleanup = !git fetch origin && git reset --hard origin/`git rev-parse --abbrev-ref HEAD`
  open = !nvim `git show --pretty="format:" --name-only HEAD`
  work = config user.email "bschmeisser@greenbits.com"
  fixup = commit --fixup
  squash = commit --squash
  review = !sh -c 'reflog-review'
[push]
  default = current
[merge]
  ff = only
[fetch]
  prune = true
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
